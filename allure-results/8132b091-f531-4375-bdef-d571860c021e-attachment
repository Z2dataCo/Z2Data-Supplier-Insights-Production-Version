        <script>


            function validateEmail($email) {
            var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,8})?$/;
            return emailReg.test($email);
        }

        $("#Username").focus();

        ////////////////////////////
        // Z2 Regular sign in form
        ////////////////////////////

        $(".signinreg #Username, .signinreg #Password").keyup(function (event) {
            if (event.keyCode === 13) {
                if (ValidLogin()) {
                    $(".signinformsubmit").click();
                }
            }
        });

        function ValidLogin() {
            $(".signinformsubmit").attr("disabled", true);
            if (!$(".signinreg #Username").val()) {
                $(".signinreg #Username").effect("shake", { times: 2 }, 200, function () {
                    $("#Username").focus();
                });
                $('.signinformsubmit').removeAttr("disabled");
                return false;
            } else if (!$(".signinreg #Password").val()) {
                $(".signinreg #Password").effect("shake", { times: 2 }, 200, function () {
                    $("#Password").focus();

                });
                $('.signinformsubmit').removeAttr("disabled");
                return false;
            } else {

                SetGoogleGeneric('Part Risk Manager', 'Auth', 'Login');
                      return true;

                       }
        }


        //$(".signinformsubmit").on("click", function() {
        //        if( !$(".signinreg #Username").val() ) {
        //           $(".signinreg #Username").effect( "shake", {times: 2}, 200, function() {
        //               $("#Username").focus();
        //          });
        //        } else if( !$(".signinreg #Password").val() ) {
        //           $(".signinreg #Password").effect( "shake", {times: 2}, 200, function() {
        //               $("#Password").focus();
        //          });
        //        } else {

        //            // TO DO:
        //            // Add login API Ajax request here.
        //            // If incorrect login, use the effects below.
        //            // If correct login, redirect to the tool.
        //            // Don't forget to remove these comments.

        //           $(".signinformsubmit").effect( "shake", {times: 2}, 200, function() {});

        //        }

        //});

        ////////////////////////////
        // Z2 SSO sign in form
        ////////////////////////////

        $(".signinsso #SSOEmail").keyup(function (event) {
            if (event.keyCode === 13) {
                $(".ssosubmit").click();
            }
        });

        $(".ssosubmit").on("click", function () {
            if (!$(".signinsso #SSOEmail").val()) {
                $(".signinsso #SSOEmail").effect("shake", { times: 2 }, 200, function () {
                    $("#SSOEmail").focus();
                });
            } else if (!validateEmail($("#SSOEmail").val())) {
                $(".signinsso #SSOEmail").effect("shake", { times: 2 }, 200, function () {
                    $("#SSOEmail").focus();
                    $(".ssoerror").slideDown();
                });
            } else {

                // TO DO:
                // Add SSO login API Ajax request here.
                // If incorrect login, use the effects below.
                // If correct login, redirect to the SSO login.
                // Don't forget to remove these comments.

                //$(".ssosubmit").effect( "shake", {times: 2}, 200, function() {
                //  $(".ssoerror").slideDown();
                //});
                let email = $(".signinsso #SSOEmail").val();

                if (email.toLowerCase().includes("dell.com") || email.toLowerCase().includes("dellteam.com") ||email.toLowerCase().includes("z2data.com")) {
                    var hreflink = $("#ssolink").attr('href');
                    console.log("hreflink", hreflink);
                    window.location.href = hreflink + "&email=" + email;
                    return;
                }
                window.location.href = "https://parts.z2data.com/account/ExternalLogin?email=" + $(".signinsso #SSOEmail").val();

            }


        });

        ////////////////////////////
        // Z2 Reset Password
        ////////////////////////////

        $("#ResetEmail").keyup(function (event) {
            if (event.keyCode === 13) {
                $(".forgotformsubmit").click();
            }
        });

        $(".forgotformsubmit").on("click", function () {
          //  $(".forgotformsubmit").attr("disabled", "true");
            // $("#forgotformsubmit").attr("disabled", true);

            if (!$("#ResetEmail").val()) {
                $("#ResetEmail").effect("shake", { times: 2 }, 200, function () {
                    $("#ResetEmail").focus();
                });
            } else if (!validateEmail($("#ResetEmail").val())) {
                $("#ResetEmail").effect("shake", { times: 2 }, 200, function () {
                    $("#ResetEmail").focus();
                    $(".emailvalerror").slideDown();
                    console.log("2");
                });
            } else {
                $(".forgotformsubmit").attr("disabled", "true");
                var token = document.getElementsByName("__RequestVerificationToken")[0].value;

                $('#gif').show();

                $.ajax({
                    type: 'POST',
                    url: "/Account/ForgotPassword",
                    data: { Email: $("#ResetEmail").val() },
                    headers: {
                        "RequestVerificationToken": token
                    },
                    //data: JSON.stringify({ Email: $("#ResetEmail").val()}),
                    //contentType: 'application/json; charset=utf-8',
                    //dataType: 'json',
                    success: function (data) {
                        $(".divforgotpw").slideUp("fast", function () {
                            $('#gif').hide();
                            $(".h1reset").hide();
                            $(".h1resetdone").show();
                            $(".emailvalerror").hide();
                            $(".signinreg .texthavingtrouble").hide();
                            $(".forgotformsubmit").removeAttr("disabled");
                        });
                    },
                    error: function (xhr, textStatus, errorThrown) {
                        $('#gif').css('visibility', 'invisible');
                        //Z2Notification(xhr.responseText, true);
                        //alert('Request Status: ' + xhr.status + ' Status Text: ' + xhr.statusText + ' ' + xhr.responseText);
                    }
                });



            }


        });


        $("#resetlink").on("click", function () {
            $(".textforgot").hide();
            $(".h1resetdone").hide();
            $(".signinerror").hide();
            $(".h1reset").slideDown();
            $(".z2signinreg").slideUp("fast", function () {
                $(".h1regsign").hide();
                $(".textreturn").show();
                $(".divforgotpw").slideDown("fast", function () {
                    $("#ResetEmail").focus();
                });
            });
        });

        $("#returnlogin").on("click", function () {
            $(".h1resetdone").hide();
            $(".textreturn").hide();
            $(".signinerror").hide();
            $(".h1regsign").slideDown();
            $(".signinreg .texthavingtrouble").show();
            $(".divforgotpw").slideUp("fast", function () {
                $(".h1reset").hide();
                $(".textforgot").show();
                $(".z2signinreg").slideDown("fast", function () {
                    $("#Username").focus();
                });
            });
        });

            $(".signtabs-sso").on("click", function () {
            $(".signtabs-sso").addClass("active");
            $(".signinerror").hide();
            $(".signtabs-reg").removeClass("active");
            $(".signinreg").slideUp("fast", function () {
            });
            $(".signinsso").slideDown();
            $("#SSOEmail").focus();
        });

        $(".signtabs-reg").on("click", function () {
            $(".signtabs-reg").addClass("active");
            $(".signinerror").hide();
            $(".signtabs-sso").removeClass("active");
            $(".signinsso").slideUp("fast", function () {
            });
            $(".signinreg").slideDown();
        });

            var isssologin = 'False';
            if (isssologin.toLocaleLowerCase() == 'true') {
                $(".signtabs-sso").click();
            }
            


        </script>





