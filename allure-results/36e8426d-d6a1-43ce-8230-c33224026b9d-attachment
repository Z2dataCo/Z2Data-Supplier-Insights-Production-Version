  <script>
    $(document).ready(function () {
      $('.btn_menu').on('click', function () {
        $(this).next('.sub_menu').slideToggle('fast');

        if ($(this).children('i').hasClass('fas fa-caret-right')) {
          $(this).children('i').removeClass('fas fa-caret-right').addClass('fas fa-caret-down');
        } else {
          $(this).children('i').removeClass('fas fa-caret-down').addClass('fas fa-caret-right');
        }
      });

      $(window).on('scroll', function () {
        if ($(window).scrollTop() >= 100) {
          $('.header-level2').addClass('z2header2_fixed');
          $('.second_logo').css('display', 'block');
          $('.second_search').css('display', 'block');
        } else {
          $('.header-level2').removeClass('z2header2_fixed');
          $('.second_logo').css('display', 'none');
          $('.second_search').css('display', 'none');
          $(".search_area").css('display', 'none');
        }
      });

      $('.close_menu').on('click', function () {
        $('.dropdown').removeClass('show');
        $('.dropdown-menu').removeClass('show');
      });

      $('.second_search').on('click', function () {
        $(".search_area").slideToggle('fast');
      });


      $('.go_scorecard').on('click', function (e) {
        e.preventDefault();
        $('#divBomDataContainer').css('display', 'none');
        $('#score_card').css('display', 'block');
      });

      $('.backward').on('click', function (e) {
        e.preventDefault();
        $('#divBomDataContainer').css('display', 'block');
        $('#score_card').css('display', 'none');
      });

      $('.show_hidetable').on('click', function () {
        $('#' + $(this).data('table')).toggle("fast");
      });

      $('.z2head-features ul li .switcher').on('click', function () {
        $('.z2head-features ul li .switcher').removeClass('bgTab');
        $(this).addClass('bgTab');
        $('.the_content').hide();
        $('#' + $(this).data('tabs')).css('display', 'block');
      });
    });
  </script>

