  <script>
    $(document).ready(function () {
      $('.btn_menu').on('click', function () {
        $(this).next('.sub_menu').slideToggle('fast');

        if ($(this).children('i').hasClass('fas fa-caret-right')) {
          $(this).children('i').removeClass('fas fa-caret-right').addClass('fas fa-caret-down');
        } else {
          $(this).children('i').removeClass('fas fa-caret-down').addClass('fas fa-caret-right');
        }
      });

      $(window).on('scroll', function () {
        if ($(window).scrollTop() >= 100) {
          $('.header-level2').addClass('z2header2_fixed');
          $('.second_logo').css('display', 'block');
        } else {
          $('.header-level2').removeClass('z2header2_fixed');
          $('.second_logo').css('display', 'none');
        }
      });

      $('.close_menu').on('click', function () {
        $('.dropdown').removeClass('show');
        $('.dropdown-menu').removeClass('show');
      });


    });

  </script>
